{
  "name": "piral-feed-service",
  "version": "1.0.0",
  "description": "Piral Feed Service - Complete seed management system for microfrontends",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build:web": "cd web && npm run build && cd ..",
    "dev:web": "cd web && npm run dev",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:web\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "migrate": "node dist/database/migrate.js",
    "seed": "node dist/database/seed.js",
    "cleanup": "ts-node scripts/cleanup-db.ts",
    "cleanup:db": "npm run cleanup"
  },
  "keywords": [
    "piral",
    "microfrontend",
    "feed-service",
    "seed-management"
  ],
  "author": "Feed Service Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.2",
    "semver": "^7.5.4",
    "sqlite3": "^5.1.6",
    "tar": "^6.1.15"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.19.25",
    "@types/semver": "^7.5.0",
    "@types/tar": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.45.0",
    "jest": "^29.6.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
